{
	"name": "server",
	"type": "module",
	"scripts": {
		"dev": "bun --watch server.ts",
		"upgrade": "npm-check-updates --upgrade --install=always --packageManager=bun",
		"check": "biome check --write --unsafe .",
		"lint": "tsc --noEmit",
		"installDeps": "bun install; bun run gen",
		"gen": "prisma generate; bun run check; prisma db push"
	},
	"dependencies": {
		"@chneau/elysia-compression": "^1.0.2",
		"@elysiajs/cors": "^1.1.0",
		"@elysiajs/jwt": "^1.1.0",
		"@elysiajs/server-timing": "^1.1.0",
		"@prisma/client": "^5.17.0",
		"elysia": "^1.1.4",
		"prisma": "^5.17.0"
	},
	"devDependencies": {
		"@types/bun": "^1.1.6",
		"prismabox": "^1.1.8",
		"typescript": "^5.5.4"
	}
}
